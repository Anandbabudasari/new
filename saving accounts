public class SavingsAccount
{ static double annualinterestrate;
  private double savingsBalance;
  void SavingsAccount(double bal){
      Savings Balance=bal;
      
  }
  double calculateMonthlyinterest(){
      double interest;
      interest=savingsBalance*(annualinterestrate/12);
      savingsBalance=savingsBalance+interest;
      return interest;
  }
  static void modify InterestRate(double new interest)
  {
      annual InterestRate=new interest();
      }
      double getbalance(){
          return savingsBalance;
      }
      public class SavingsAccount3{
          public static void main(String[] args){
              SavingsAccount.saver1=new SavingsAccount();
              SavingsAccount.saver2=new SavingsAccount();
              saver1.SavingsAccount(2000);
              saver2.SavingsAccount(3000);
              SavingsAccount.modifyInterestRate(0.04);
              saver1.calculateMonthlyinterest();
              saver2.calculateMonthlyinterest();
              System.out.System.out.println("Balance of saver1 is:"+saver1.getbalance());
              System.out.println("Balance of saver2 is :"+saver2.getbalance());
              
              SavingsAccount.modifyInterestRate(0.05);
              saver1.calculateMonthlyinterest();
              saver2.calculateMonthlyinterest();
              System.out.println("Balanceof saver1 is:"+saver1.getbalance());
              System.out.println("Balance of saver2 is:"+saver2.getbalance());
              
          }
          
          
      }
      
     
       
    
}
